{
  "name": "boilerplate-cra-nodejs",
  "description": "A website boilerplate that uses create-react-app and a custom Node API",
  "version": "0.0.0",
  "scripts": {
    "start": "node server/index.js",
    "build": "cd client/ && npm install && npm run build",
    "dev": "run-p dev:**",
    "dev:server": "nodemon server/index.js",
    "dev:app": "cd client/ && npm start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app",
    "react"
  ],
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": "14.7.0",
    "npm": "6.14.7"
  },
  "author": {
    "name": "Shinell Manwaring"
  },
  "license": "MIT"
}
